let Higher_CShar = 0
let Higher_C = 1046
Higher_CShar = 1109
Higher_D = 1175
Higher_DShar = 1244
Higher_E = 1318
let Higher_F = 1397
Higher_FShar = 1480
let Higher_G = 1661
let Higher_A = 1760
dottedQuarterNote = 473
dottedHalfNote = 947
tiedFourtoHalf = 1420
trill = 39
// separate here
music.setTempo(190)
forever(function () {
    section1()
    music.playTone(988, dottedQuarterNote)
    music.playTone(659, dottedQuarterNote)
    music.playTone(831, music.beat(BeatFraction.Whole))
    music.playTone(Higher_DShar, dottedQuarterNote)
    music.playTone(Higher_E, music.beat(BeatFraction.Quarter))
    music.playTone(Higher_DShar, music.beat(BeatFraction.Quarter))
    music.playTone(Higher_CShar, music.beat(BeatFraction.Double))
    section1()
    music.playTone(988, dottedQuarterNote)
    music.playTone(Higher_CShar, dottedQuarterNote)
    music.playTone(831, music.beat(BeatFraction.Whole))
    music.playTone(Higher_DShar, dottedQuarterNote)
    music.playTone(Higher_E, music.beat(BeatFraction.Quarter))
    music.playTone(Higher_DShar, music.beat(BeatFraction.Quarter))
    music.playTone(Higher_CShar, music.beat(BeatFraction.Double))
    section2()
    section2()
    section3()
    section3()
    section4()
    section4()
    section5()
    section5()
})
