function thatOnePartinEndPart () {
    music.playTone(831, dottedEightNote)
    music.playTone(831, music.beat(BeatFraction.Quarter))
    music.rest(music.beat(BeatFraction.Half))
    music.playTone(831, music.beat(BeatFraction.Half))
    music.playTone(932, dottedEightNote)
    music.playTone(932, music.beat(BeatFraction.Quarter))
    music.rest(music.beat(BeatFraction.Half))
    music.playTone(932, music.beat(BeatFraction.Half))
}
let eigthTriplet = 0
let dottedEightNote = 0
let quarterDottedNote = 0
let Higher_E = 0
let Higher_D2 = 0
let Higher_D = 0
let Higher_C2 = 0
let Higher_C = 0
Higher_C = 1046
Higher_C2 = 1109
Higher_D = 1175
Higher_D2 = 1244
Higher_E = 1318
let Higher_F = 1397
let Higher_G = 1661
let Higher_A = 1760
quarterDottedNote = 500
dottedEightNote = 250
eigthTriplet = 55
// separate here
music.setTempo(180)
forever(function () {
    introSegment()
    introSegment()
    introSegment()
    music.playTone(294, music.beat(BeatFraction.Half))
    music.playTone(440, music.beat(BeatFraction.Half))
